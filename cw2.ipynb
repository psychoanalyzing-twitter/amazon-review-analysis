     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "train['Lemmatized Reviews'] = train['Tagged Reviews'].apply(lambda x: [wnl.lemmatize(word[0], pos=word[1]) if word[1] is not None else wnl.lemmatize(word[0]) for word in x ])\n",
    "train['Lemmatized Reviews'].head()"
